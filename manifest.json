{
    "action": {
        "default_popup": "popup.html"
    },
    "description": "A super simple chrome extension to sort and control duplicated tabs",
    "icons": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png"
    },
    "manifest_version": 3,
    "name": "No more tabs",
    "options_page": "settings.html",
    "permissions": [
        "storage",
        "tabs",
        "windows"
    ],
    "version": "0.3"
}