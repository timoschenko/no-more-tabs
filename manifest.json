{
    "manifest_version": 3,
    "name": "No more tabs",
    "version": "0.1",
    "description": "A super simple chrome extension to sort and control duplicated tabs",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "32": "images/favicon-32x32.png",
        "16": "images/favicon-16x16.png"
    }
}